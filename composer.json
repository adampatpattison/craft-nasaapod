{
    "name": "adampatpattison/craft-nasaapod",
    "description": "Adds a simple widget which pulls the NASA Astronomy Picture of the Day in the dashboard",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "NASA apod",
        "widget",
        "photos"
    ],
    "support": {
        "docs": "https://github.com/adampatpattison/craft-nasaapod/blob/master/README.md",
        "issues": "https://github.com/adampatpattison/craft-nasaapod/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Pat Pattison",
            "homepage": "https://www.adampatpattison.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "adampatpattison\\nasaapod\\": "src/"
        }
    },
    "extra": {
        "name": "NASA Astronomy Picture of the Day",
        "handle": "nasa-apod",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/adampatpattison/craft-nasaapod/master/CHANGELOG.md",
        "class": "adampatpattison\\nasaapod\\NasaApod"
    }
}
